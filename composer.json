{
  "name": "eveschipfer/sdk-mp",
  "description": "Mercado Pago PHP SDK",
  "type": "library",
  "homepage": "https://github.com/eveschipfer/sdk-php",
  "license": "MIT",
  "version": "1.0.2",
  "config": {
    "platform": {
        "php": "8.1"
    }
  },
  "require": {
    "php": ">=8.1",
    "doctrine/common": "^2.6 || ^3.0",
    "doctrine/annotations": "^1.8"
  },
  "require-dev": {
    "symfony/yaml": "~2.5",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "2.8.1",
    "sebastian/phpcpd": "*",
    "doctrine/orm": "~2.3",
    "vlucas/phpdotenv": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "MercadoPago\\": [
        "src/MercadoPago/"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    { "packagist": false }
  ]
}
